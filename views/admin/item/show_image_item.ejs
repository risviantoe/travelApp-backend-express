<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="nav-home-tab">
  <div class="row">
    <% for(let i=0; i < item.imageId.length; i ++) { %>
      <div class="col-md-4">
        <div class="card shadow mb-4">
          <div class="card-body">
            <img src="http://localhost:3001/<%= item.imageId[i].imageUrl %>" alt="image" style="width: 100%">
          </div>
        </div>
      </div>
    <% } %>
  </div>
  <a href="/admin/item" class="btn btn-danger my-3">Back</a>
</div>